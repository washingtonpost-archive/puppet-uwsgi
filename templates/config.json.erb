{
    "uwsgi":  {
        "socket": "/tmp/uwsgi.<%= name %>.sock",
        "master": true,
        "processes": <%= processes %>,
        "buffer-size": <%= buffer_size %>,
        <% params.each do |key, value| %>"<%= key %>": <%= value %>,
        <% end %>
        <% if zerg %>
        "vassal-sos-backlog": "<%= vassal_sos_backlog %>",
        "zerg-server": "/tmp/uwsgi.<%= name %>.zerg.sock",<% end %>
        "workers": <%= workers %>
    }<% if zerg %>,"zerg":  {
        "socket": "/tmp/uwsgi.<%= name %>.zerg.sock",
        "master": true,
        <% params.each do |key, value| %>"<%= key %>": <%= value %>,
        <% end %>
        "processes": <%= processes %>,
        "buffer-size": <%= buffer_size %>,
        "disable-logging": <%= disable_logging %>,
        "idle": <%= idle %>,
        "die-on-idle": <%= die_on_idle %>
    }
    <% end %>
}


